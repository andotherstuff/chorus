{
  "mcpServers": {
    "nostr": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "xjsr", "@nostrbook/mcp"]
    },
    "puppeteer-server": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    },
    "Sentry": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.sentry.dev/mcp"]
    }
  }
}
